<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:padding="5dp" android:shape="rectangle">
            <solid android:color="@color/colorPrimaryAqua"/>
            <stroke android:width="1dp" android:color="@color/colorPrimaryAqua"/>

        </shape>
    </item>
    <item android:state_focused="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:padding="5dp" android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark"/>
            <stroke android:width="1dp" android:color="@color/colorPrimaryDark"/>


        </shape>
    </item>
</selector>